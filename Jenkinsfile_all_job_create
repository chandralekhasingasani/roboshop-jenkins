pipeline{
    agent 'agent'
    stages{
        stage('checkout'){
            steps{
                sh 'ls -ltr'
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'agent', url: 'https://github.com/chandralekhasingasani/roboshop-jenkins.git']])
            }

        }
        stage('create'){
            steps{
                sh 'ls -ltr'
                sh 'ansible-playbook main.yml'
            }

        }
    }
}